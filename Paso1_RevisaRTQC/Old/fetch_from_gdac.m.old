function fetch_from_gdac(wmonum,GDAC,QC1)
    %function fetch_from_gdac(wmonum)
    %  1) copies profile netcdf files from GDAC_MIRROR and places in local QC1 directory.
    %  2) adds ARSQ stamp
    
    fltdir  = [GDAC,num2str(wmonum),'/profiles/'];
    
    if exist(fltdir) == 7 %& delay_found ==1;
        %get directory list of netcdf files
        w=dir(fltdir);
        % sort names because matlab doesn't understand english
        tw=strvcat(w.name);
        wqc=sortrows(tw);
        nqc = length(strmatch('R',wqc(:,1)));
        disp([num2str(wmonum),'>>>>> Moving Float RAW netcdf files found in ',fltdir]);
        disp([blanks(14),'Total Profiles: ',num2str(nqc)])
        
        iqc = strmatch('R',wqc(:,1));
        for ii = 1:nqc
            fltdir  = [GDAC,num2str(wmonum),'/profiles/'];
            outdir = [QC1,num2str(wmonum),'/'];
            if exist(outdir) ~=7
                disp(['Making directory: ',outdir])
                eval(['!mkdir ',outdir])
            end
            flname = wqc(iqc(ii),:);
            ncfile  = fullfile(fltdir,flname);
            outfile = fullfile(outdir,flname);
            sw = dir(ncfile);
            sourcedate1 = sw.datenum;
            sw = dir(outfile);
            if ~isempty(sw)
                sourcedate2 = sw.datenum;
            else
                sourcedate2 = 0;
            end
            
            if sourcedate2 + .0001 > sourcedate1
                disp([num2str(wmonum),': ',flname, ' on ',GDAC,' partition is older'])
            else
                disp([num2str(wmonum),'  Moving: ',ncfile]);
                eval(['!cp -f -p ',ncfile,' ',outfile])
                
                % add the ARSQ flag
                ncid= netcdf.open(outfile, 'WRITE');
                
                % write out history statement
                histdate = sprintf('%04d%02d%02d%02d%02d%02d',round(clock));
                
                nhistory_id =  netcdf.inqdimID(ncid,'N_HISTORY');
                [foo, N_HISTORY] = netcdf.inqdim(ncid,nhistory_id);
                
                % note we are increminting history count by one, but netcdf indexing starts at
                % zero instead of one.
                varid = netcdf.inqVarID(ncid,'HISTORY_INSTITUTION');
                netcdf.putVar(ncid,varid,[0 0 N_HISTORY],[4 1 1],'IEO')
                
                varid = netcdf.inqVarID(ncid,'HISTORY_STEP');
                netcdf.putVar(ncid,varid,[0 0 N_HISTORY],[4 1 1],'ARSQ')
                
                varid = netcdf.inqVarID(ncid,'HISTORY_SOFTWARE');
                netcdf.putVar(ncid,varid,[0 0 N_HISTORY],[4 1 1],'WHGE')
                
                varid = netcdf.inqVarID(ncid,'HISTORY_SOFTWARE_RELEASE');
                netcdf.putVar(ncid,varid,[0 0 N_HISTORY],[4 1 1],'V0.1')
                
                varid = netcdf.inqVarID(ncid,'HISTORY_DATE');
                netcdf.putVar(ncid,varid,[0 0 N_HISTORY],[14 1 1],histdate)
                
                varid = netcdf.inqVarID(ncid,'HISTORY_ACTION');
                netcdf.putVar(ncid,varid,[0 0 N_HISTORY],[2 1 1],'QC')
                
                netcdf.close(ncid);
            end
        end
    end